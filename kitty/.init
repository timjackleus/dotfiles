# Symlink config (will force if file already exist)
ln -s $PWD/kitty/kitty.conf ~/.config/kitty/kitty.conf


ln -s $PWD/kitty/themes ~/.config/kitty/

# Copy better program icon
cp $PWD/kitty/kitty-dark.icns ~/.config/kitty/

# refresh the dock
rm /var/folders/*/*/*/com.apple.dock.iconcache; killall Dock
